{
    "manifest_version": 2,
    "name": "Extractor",
    "version": "0.2",
    "description": "JTBC 자막 추출기",
    
    "browser_action": {
        "default_popup": "main.html"
    },
    "content_scripts": [{
	"all_frames": true,
	"matches": ["*://tv.jtbc.co.kr/replay/*"],
	"run_at": "document_start",
	"js": ["function.js"]
    }]
}
